{
  "name": "powerbi-client-react-monorepo",
  "private": true,
  "workspaces": [
    "packages/react-client",
    "packages/token-api",
    "packages/powerbi-secured-embed"
  ],
  "scripts": {
    "start": "concurrently \"npm:start --workspace=token-api\" \"npm:start --workspace=react-client\"",
    "dev": "npm run start:secured-embed",
    "start:secured-embed": "npm start --workspace=powerbi-secured-embed"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
